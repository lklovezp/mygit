<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hnjz.quartz.po.Sms" table="T_SMS">

		<!-- A 32 hex character is our surrogate key. It's automatically
			generated by Hibernate with the UUID pattern. -->
		<id name="id" type="string" unsaved-value="null">
			<column name="ID" sql-type="char(32)" not-null="true" />
			<generator class="uuid.hex" />
		</id>
		<property column="JSR_NAME_" name="jsrName" type="string" />
		<property column="JSR_PHONE_" name="jsrPhone" type="string" />
		<property column="SMS_CONTENT_" name="smsContent" type="string" />
		<property column="MEMO_" name="memo" type="string" />
		<property column="SEND_COUNT_" name="sendCount" type="integer" />
		<property column="UPLOAD_TIME_" name="uploadTime" type="date" />
		<property column="SEND_TIME_" name="sendTime" type="date" />
			
	</class>
</hibernate-mapping>