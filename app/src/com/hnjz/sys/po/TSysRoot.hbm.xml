<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hnjz.sys.po.TSysRoot" table="T_SYS_ROOT">
		<id name="id" type="string" unsaved-value="null">
			<column name="ID_" sql-type="char(32)" not-null="true" />
			<generator class="uuid.hex" />
		</id>
		
	    <property column="PID_" name="pid" not-null="false" type="string" />
        <property column="NAME_" name="name" not-null="false" type="string" />
         <property column="STATE_" name="state" not-null="false" type="string" />
         <property column="CREATETIME_" name="createTime" not-null="false" type="string" />
         <property column="CREATEUSERID_" name="createUserId" not-null="false" type="string" />
       
		<property column="BAK_" name="bak" not-null="false" type="string" />
	</class>
</hibernate-mapping>
