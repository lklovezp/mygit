<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hnjz.sys.po.SysExport" table="SYS_EXPORT">

		<!-- A 32 hex character is our surrogate key. It's automatically
			generated by Hibernate with the UUID pattern. -->
		<id name="id" type="string" unsaved-value="null">
			<column name="ID_" sql-type="char(32)" not-null="true" />
			<generator class="uuid.hex" />
		</id>

		<property column="TABLE_" name="tablename" type="string" />
		<property column="NAME_" name="name" type="string" />
		<property column="NOTE_" name="note" type="string" />
		<property column="DICCODE_" name="diccode" type="string" />
		<property column="FIELDTYPE_" name="fieldtype" type="string" />
		<property column="FIELDLENGTH_" name="fieldlength" type="int" />
		<property column="PARAMETERCODE_" name="parameterCode" type="string" />
		<property column="DEFAULTCODE_" name="defaultCode" type="string" />
		<property column="ISREQUIRED_" name="isRequired" type="string" />
	</class>
</hibernate-mapping>
