<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.hnjz.wf.entity.WfApplyStep" table="T_BIZ_WFNODEINFO">

		<!-- A 32 hex character is our surrogate key. It's automatically
			generated by Hibernate with the UUID pattern. -->
		<id name="id" type="string" unsaved-value="null">
			<column name="ID_" sql-type="char(32)" not-null="true" />
			<generator class="uuid.hex" />
		</id>
		<version column="VERSION_" name="version" />
		<property column="ISACTIVE_" name="isActive" type="string" />
		<property column="CREATED_" name="createTime" type="timestamp" />
		<many-to-one column="CREATEBY_" name="createUser" class="com.hnjz.sys.po.TSysUser"/>
		<property column="UPDATED_" name="updateTime" type="timestamp" />
		<many-to-one column="UPDATEBY_" name="updateUser" class="com.hnjz.sys.po.TSysUser"/>
		<property column="SORT_NUM_" name="order" type="integer" />		

		<property column="APPLY_ID_" name="applyId" type="string" />
		<property column="EXEC_USER_" name="execUser" type="string" />
		<property column="TASK_ID_" name="taskId" type="string" />
		<property column="TASK_NAME_" name="taskName" type="string" />
		<property column="OPINION_" name="opinion" type="string" />
		<property column="RESULT_" name="result" type="string" />
		<property column="NOTE_" name="note" type="string" />
		
	</class>

</hibernate-mapping>
