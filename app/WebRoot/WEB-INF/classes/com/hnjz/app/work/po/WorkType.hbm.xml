<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.hnjz.app.work.po.WorkType" table="WORK_TYPE">

		<!-- A 32 hex character is our surrogate key. It's automatically
			generated by Hibernate with the UUID pattern. -->
		<id name="id" type="string" unsaved-value="null">
			<column name="ID_" sql-type="char(32)" not-null="true" />
			<generator class="uuid.hex" />
		</id>
		<property column="CODE_" name="code" type="string" />
		<property column="NAME_" name="name" type="string" />
		<property column="DESC_" name="note" type="string" />
		<property column="PID_" name="pid" type="string" />
		<property column="ORDER_" name="order" not-null="true"
			type="integer" />
		<property column="ISCREATE_JJRW_" name="isjjrw" type="string" />
		<property column="SHJB_" name="shjb" type="string" />
		<property column="URL_" name="url" type="string" />
		<property column="URL2_" name="url2" type="string" />
		<property column="SJURL_" name="sjurl" type="string" />
		<property column="GZLIU_" name="gzliu" type="string" />
		<many-to-one name="creater" class="com.hnjz.sys.po.TSysUser"
			column="CREATEBY_" cascade="none" />
		<many-to-one name="updateby" class="com.hnjz.sys.po.TSysUser"
			column="UPDATEBY_" cascade="none" />
		<property column="UPDATED_" name="updateTime" type="timestamp" />
		<property column="ISACTIVE_" name="isActive" not-null="true"
			type="string" />
		<property column="CREATED_" name="createTime" not-null="true"
			type="timestamp" />
	</class>

</hibernate-mapping>
