<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hnjz.app.work.po.TBizTaskrecordans" table="T_BIZ_TASKRECORDANS">
		<id name="id" type="string" unsaved-value="null">
			<column name="ID_" sql-type="char(32)" not-null="true" />
			<generator class="uuid.hex" />
		</id>
		<version column="VERSION_" name="version" />

		<property column="TASKID_" name="taskid" not-null="true" type="string" />
		<property column="RECORDID_" name="recordid" not-null="false" type="string" />
		<property column="CONTENT_" name="content" not-null="true" type="string" />
		<property column="DANR_" name="danr" not-null="false" type="string" />
		<property column="KCXWBJ_" name="kcxwbj" not-null="true" type="string" />
		<property column="WTTYPE_" name="wttype" not-null="true" type="string" />
		<property column="ORDERBY_" name="orderby" not-null="true" type="integer" />
		<property column="ISDEL_" name="isdel" not-null="false" type="string" />
		<property column="ISACTIVE_" name="isActive" not-null="true" type="string" />
		<property column="UPDATED_" name="updated" not-null="true" type="timestamp" />
		<many-to-one name="updateby" column="UPDATEBY_" class="com.hnjz.sys.po.TSysUser" not-null="true"></many-to-one>
		<property column="CREATED_" name="created" not-null="true" type="timestamp" />
		<many-to-one name="createby" column="CREATEBY_" class="com.hnjz.sys.po.TSysUser" not-null="true"></many-to-one>
	</class>
</hibernate-mapping>
