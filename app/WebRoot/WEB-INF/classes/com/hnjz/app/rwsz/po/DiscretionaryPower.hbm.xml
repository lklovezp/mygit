<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.hnjz.app.rwsz.po.DiscretionaryPower" table="T_DISCRETION">
		<!-- A 32 hex character is our surrogate key. It's automatically
			generated by Hibernate with the UUID pattern. -->
		<id name="id" type="string" unsaved-value="null">
			<column name="ID_" sql-type="char(32)" not-null="true" />
			<generator class="uuid.hex" />
		</id>
		<property column="NAME_" name="name" type="string" />
		<property column="CONTENT_" name="content" type="string" />
		<property column="GJC_" name="gjc" type="string" />
		<property column="PID_" name="pid" type="string" />
		<property column="ILLEGAL_TYPE_ID_" name="illegalTypeId" type="string" />
		<property column="ILLEGAL_TYPE_NAME_" name="illegalTypeName" type="string" />
		<property column="CODE_" name="code" type="string" />
	</class>

</hibernate-mapping>
