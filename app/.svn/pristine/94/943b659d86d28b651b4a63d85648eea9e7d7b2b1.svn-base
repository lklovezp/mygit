package com.hnjz.sys.user;

import org.springframework.web.multipart.MultipartFile;

/**
 * 用户的表单对象
 * 
 * @author wumi
 * @version $Id: UserForm.java, v 0.1 Jan 17, 2013 9:29:06 AM wumi Exp $
 */
public class UserForm {

	private String id;

	private String username; // 用户名称

	private String name; // 实际名称

	private String password; // 密码

	private String oldPas; // 旧密码

	private String confirmPassword;

	public String isActive; // 有效无效

	public String issys; // 是否管理员

	public String workmobile; // 工作手机号
	
	private String personmobile; // 个人手机号

	public String role; // 角色

	private String gxOrg; // 所属部门

	private String gxOrgName; // 所属部门

	private Integer orderby;

	private String zfzh; // 执法证号

	private String bizType; // 业务类型

	private String filePath; // 照片
	private String isZaiBian;
	private String isRecWork;
	private MultipartFile file;
	
	
	private String fid;
	
	public String getFid() {
		return fid;
	}

	public void setFid(String fid) {
		this.fid = fid;
	}

	public String group; // 会商接收人分组
   
	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getOldPas() {
		return oldPas;
	}

	public void setOldPas(String oldPas) {
		this.oldPas = oldPas;
	}

	public String getConfirmPassword() {
		return confirmPassword;
	}

	public void setConfirmPassword(String confirmPassword) {
		this.confirmPassword = confirmPassword;
