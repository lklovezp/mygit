public class QuarterForm {
	
	private String tasktypeCode;
	
	private String taskname;
	
	private Integer tasksum;
	
	private Integer fsgs;
	private Integer fscs;
	private Integer fsrc;
	
	private Integer fqgs;
	private Integer fqcs;
	private Integer fqrc;
	
	private Integer zsgs;
	private Integer zscs;
	private Integer zsrc;
	
	private Integer gfgs;
	private Integer gfcs;
	private Integer gfrc;
	
	private Integer wrygs;
	
	private Integer xckczcs;
	
	private Integer xckczrc;
	private Integer xczrs;
	
	private Integer qtjcs;
	
	/** 其他检查备注 **/
	private String qtjcRemark;
	
	public QuarterForm(){
		
	}
	
	public QuarterForm(String taskname, Integer tasksum, Integer fsgs, Integer fqgs, Integer zsgs, Integer gfgs, Integer fscs, Integer fqcs, Integer zscs, Integer gfcs, Integer fsrc, Integer fqrc, Integer zsrc, Integer gfrc, Integer xczrs, String tasktypeCode){
		this.taskname = taskname;
		this.tasksum = tasksum;
		this.fsgs = fsgs;
		this.fqgs = fqgs;
		this.zsgs = zsgs;
		this.gfgs = gfgs;
		this.fscs = fscs;
		this.fqcs = fqcs;
		this.zscs = zscs;
		this.gfcs = gfcs;
		this.fsrc = fsrc;
		this.fqrc = fqrc;
		this.zsrc = zsrc;
		this.gfrc = gfrc;
		this.wrygs = this.fsgs + this.fqgs + this.zsgs + this.gfgs;
		this.xckczcs = this.fscs + this.fqcs + this.zscs + this.gfcs;
		//this.xckczrc = this.fsrc + this.fqrc + this.zsrc + this.gfrc;
		this.xckczrc = xczrs;
		this.tasktypeCode = tasktypeCode;
	}

	public String getTaskname() {
		return taskname;
	}

	public void setTaskname(String taskname) {
		this.taskname = taskname;
	}


	public Integer getTasksum() {
		return tasksum;
	}

	public void setTasksum(Integer tasksum) {
		this.tasksum = tasksum;
	}

	public Integer getFsgs() {
		return fsgs;
	}

	public void setFsgs(Integer fsgs) {
		this.fsgs = fsgs;
	}

	public Integer getFqgs() {
		return fqgs;
	}

	public void setFqgs(Integer fqgs) {
		this.fqgs = fqgs;
	}

	public Integer getZsgs() {
		return zsgs;
	}

	public void setZsgs(Integer zsgs) {
		this.zsgs = zsgs;
	}

	public Integer getGfgs() {
		return gfgs;
	}

	public void setGfgs(Integer gfgs) {
		this.gfgs = gfgs;
	}

	public Integer getFscs() {
		return fscs;
	}

	public void setFscs(Integer fscs) {
		this.fscs = fscs;
	}

	public Integer getFqcs() {
		return fqcs;
	}

	public void setFqcs(Integer fqcs) {
		this.fqcs = fqcs;
	}

	public Integer getZscs() {
		return zscs;
	}

	public void setZscs(Integer zscs) {
		this.zscs = zscs;
	}

	public Integer getGfcs() {
		return gfcs;
	}

	public void setGfcs(Integer gfcs) {
		this.gfcs = gfcs;
	}

	public Integer getFsrc() {
		return fsrc;
	}

	public void setFsrc(Integer fsrc) {
		this.fsrc = fsrc;
	}

	public Integer getFqrc() {
		return fqrc;
	}

	public void setFqrc(Integer fqrc) {
		this.fqrc = fqrc;
	}

	public Integer getZsrc() {
		return zsrc;
	}

	public void setZsrc(Integer zsrc) {
		this.zsrc = zsrc;
	}

	public Integer getGfrc() {
		return gfrc;
	}

	public void setGfrc(Integer gfrc) {
		this.gfrc = gfrc;
	}

	public Integer getWrygs() {
		return wrygs;
	}

	public void setWrygs(Integer wrygs) {
		this.wrygs = wrygs;
	}

	public Integer getXckczcs() {
		return xckczcs;
	}

	public void setXckczcs(Integer xckczcs) {
		this.xckczcs = xckczcs;
	}

	public Integer getXckczrc() {
		return xckczrc;
	}

	public void setXckczrc(Integer xckczrc) {
		this.xckczrc = xckczrc;
	}

	public Integer getQtjcs() {
		return qtjcs;
	}

	public void setQtjcs(Integer qtjcs) {
		this.qtjcs = qtjcs;
	}

	public String getTasktypeCode() {
		return tasktypeCode;
	}

	public void setTasktypeCode(String tasktypeCode) {
		this.tasktypeCode = tasktypeCode;
	}

	public String getQtjcRemark() {
		return qtjcRemark;
	}

	public void setQtjcRemark(String qtjcRemark) {
		this.qtjcRemark = qtjcRemark;
	}

	public Integer getXczrs() {
		return xczrs;
	}

	public void setXczrs(Integer xczrs) {
		this.xczrs = xczrs;
	}

}
