package com.hnjz.sys.tasktype;

/**
 * 任务类型的Form
 * 
 * @author wumi
 * @version $Id: AreaForm.java, v 0.1 2013-3-25 下午03:33:13 wumi Exp $
 */
public class TaskTypeForm {

	private String id;
	
	private String name;
	
	private String code;

	private String pid;
	
	private String lawobjtype;
	
	private String illegaltype;

	private String dotemplate;
	
	private String describe;
	
	private Integer orderby;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}
	
	public String getPid() {
		return pid;
	}

	public void setPid(String pid) {
		this.pid = pid;
	}

	public String getLawobjtype() {
		return lawobjtype;
	}

	public void setLawobjtype(String lawobjtype) {
		this.lawobjtype = lawobjtype;
	}

	public String getIllegaltype() {
		return illegaltype;
	}

	public void setIllegaltype(String illegaltype) {
		this.illegaltype = illegaltype;
	}

	public String getDotemplate() {
		return dotemplate;
	}

	public void setDotemplate(String dotemplate) {
		this.dotemplate = dotemplate;
	}

	public String getDescribe() {
		return describe;
	}

	public void setDescribe(String describe) {
		this.describe = describe;
	}

	public Integer getOrderby() {
		return orderby;
	}

	public void setOrderby(Integer orderby) {
		this.orderby = orderby;
	}
}
