<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hnjz.app.work.po.TBizBlwtx" table="T_BIZ_BLWTX">
		<id name="id" type="string" unsaved-value="null">
			<column name="ID_" sql-type="char(32)" not-null="true" />
			<generator class="uuid.hex" />
		</id>

		<property column="RWID_" name="rwid" not-null="false" type="string" />
		<property column="WTXID_" name="wtxid" not-null="false" type="string" />
		<property column="CONTENT_" name="content" not-null="false" type="string" />
	</class>
</hibernate-mapping>
