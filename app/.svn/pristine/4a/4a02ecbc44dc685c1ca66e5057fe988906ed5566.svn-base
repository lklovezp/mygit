.homeDiv dl dd .con ul li a:hover{ text-decoration:none; color:#3F96CB;}
.homeDiv dl dd .con ul li span{ float:right;}
.homeDiv dl dd .con ul li span font{ display:inline-block; width:16px; text-align:center;}
