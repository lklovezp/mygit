img{ border:0px;}
ul,li{ list-style:none; margin:0; padding:0;}
h1,h2,h3,h4,h5,h6{ margin:0; padding:0; font-size:12px; font-weight:normal;}
a{ color:#333333; text-decoration:none;}
a:hover{ color:#333333; text-decoration:underline;}
.clearfix{*zoom:1;}
.clearfix:before{ content:""; height:0; line-height:0; display:table;}
.clearfix:after{ content:""; height:0; line-height:0; display:table; clear:both;}

.homeDiv dl dd{ width:50%; float:left;}
.homeDiv dl dd .con{ height:246px; overflow:hidden; border:1px solid #e6e6e6; margin:20px 6px 0px 16px; background:url(images/tit_bg_line2.png) repeat-x;}
.homeDiv dl dd .con h2{ height:36px; padding:0px 14px 0px 37px; line-height:35px; color:#666; font-size:14px; font-family:"Microsoft YaHei"; font-weight:bold; overflow:hidden;}
.homeDiv dl dd .con h2 a{ display:inline-block; width:47px; height:11px; background:url(images/more.png) no-repeat; float:right; margin-top:13px; text-indent:-9999px; cursor:pointer;}
.homeDiv dl dd.dd1 h2{ background:url(images/tit_ico04.png) 9px 6px no-repeat;}
.homeDiv dl dd.dd2 h2{ background:url(images/tit_ico03.png) 9px 7px no-repeat;}
.homeDiv dl dd.dd3 h2{ background:url(images/tit_ico05.png) 9px 7px no-repeat;}
.homeDiv dl dd.dd4 h2{ background:url(images/tit_ico02.png) 9px 7px no-repeat;}
.homeDiv dl dd .con ul{ margin:0px 1px;}
.homeDiv dl dd .con ul li{ height:34px; line-height:34px; overflow:hidden;  padding-left:23px; padding-right:12px; border-bottom:1px dotted #e4e1e1; background:url(images/li_ico.png) 9px 14px no-repeat;}
