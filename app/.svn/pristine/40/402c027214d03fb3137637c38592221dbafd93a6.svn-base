package com.hnjz.sys.function;

/**
 * 
 * @author wumi
 * @version $Id: FunOp.java, v 0.1 Jan 18, 2013 2:09:44 PM wumi Exp $
 */
public class FunOp {

    private String opid;

    private String name;

    private String eventName;

    private String linkAddr;

    private String isTableShow;

    public String getOpid() {
        return opid;
    }

    public void setOpid(String opid) {
        this.opid = opid;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getEventName() {
        return eventName;
    }

    public void setEventName(String eventName) {
        this.eventName = eventName;
    }

    public String getLinkAddr() {
        return linkAddr;
    }

    public void setLinkAddr(String linkAddr) {
        this.linkAddr = linkAddr;
    }

    public String getIsTableShow() {
        return isTableShow;
    }

    public void setIsTableShow(String isTableShow) {
        this.isTableShow = isTableShow;
    }

}
