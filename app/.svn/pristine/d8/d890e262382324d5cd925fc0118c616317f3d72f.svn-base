<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hnjz.sys.po.Dbdic" table="SYS_QUERY_DBDIC">

		<!-- A 32 hex character is our surrogate key. It's automatically
			generated by Hibernate with the UUID pattern. -->
		<id name="id" type="string" unsaved-value="null">
			<column name="ID_" sql-type="char(32)" not-null="true" />
			<generator class="uuid.hex" />
		</id>

		<property column="TABLE_NAME_" name="tableName" type="string" />
		<property column="COLOUMN_NAME_" name="coloumnName"
			type="string" />
		<property column="TABLE_NOTE_" name="tableNote" type="string" />
		<property column="COLOUMN_NOTE_" name="coloumnNote"
			type="string" />
		<property column="COLTYPE_" name="colType" type="string" />
		<property column="COLOUMNDATA_" name="colScript" type="string" />

		<many-to-one name="creater" class="com.hnjz.sys.po.User"
			column="CREATOR_" />
		<property column="CREATETIME_" name="createTime"
			type="timestamp" />
	</class>
</hibernate-mapping>
