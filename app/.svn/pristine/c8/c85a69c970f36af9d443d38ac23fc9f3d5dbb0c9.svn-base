package com.hnjz.sys.industry;

/**
 * 行业的Form
 * 
 * @author wumi
 * @version $Id: IndustryForm.java, v 0.1 2013-3-25 下午03:33:13 wumi Exp $
 */
public class IndustryForm {

	private String id;

	private String name;
    
	private String code;
	
	private String lawobjtype;

	private String isActive;
	
	private String describe;
	
	private String tolawobjtype;
	
	private int orderby;
	
	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getLawobjtype() {
		return lawobjtype;
	}

	public void setLawobjtype(String lawobjtype) {
		this.lawobjtype = lawobjtype;
	}

	public String getIsActive() {
		return isActive;
	}

	public void setIsActive(String isActive) {
		this.isActive = isActive;
	}
	
	public String getDescribe() {
		return describe;
	}

	public void setDescribe(String describe) {
		this.describe = describe;
	}
	
	public void setTolawobjtype(String tolawobjtype) {
		this.tolawobjtype = tolawobjtype;
	}
	
	public String getTolawobjtype() {
		return tolawobjtype;
	}

	public void setOrderby(int orderby) {
		this.orderby = orderby;
	}

	public int getOrderby() {
		return orderby;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}
	
}
