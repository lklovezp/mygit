package com.hnjz.app.work.rwgl;

/**
 * 任务派发的表单对象
 * 作者：xb
 * 生成日期：Mar 9, 2015
 * 功能描述：

 *
 */
public class RwpfForm {

	/**
	 * 任务id
	 */
	private String id;

	/**
	 * 任务名称
	 */
	private String rwmc;

	/**
	 * 主要内容
	 */
	private String zynr;
	
	/**
	 * 任务生成时间
	 */
	private String rwscsj;
	
	/**
	 * 要求完成时限
	 */
	private String yqwcsx;
	
	/**
	 * 接受人
	 */
	private String jsr;
	
	/**
	 * 派发人
	 */
	private String pfr;
	
	/**
	 * 批示意见
	 */
	private String psyj;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getRwmc() {
		return rwmc;
	}

	public void setRwmc(String rwmc) {
		this.rwmc = rwmc;
	}

	public String getZynr() {
		return zynr;
	}

	public void setZynr(String zynr) {
		this.zynr = zynr;
	}

	public String getRwscsj() {
		return rwscsj;
	}

	public void setRwscsj(String rwscsj) {
		this.rwscsj = rwscsj;
	}

	public String getYqwcsx() {
		return yqwcsx;
	}

	public void setYqwcsx(String yqwcsx) {
		this.yqwcsx = yqwcsx;
	}

	public String getJsr() {
		return jsr;
	}

	public void setJsr(String jsr) {
		this.jsr = jsr;
	}

	public String getPfr() {
		return pfr;
	}

	public void setPfr(String pfr) {
		this.pfr = pfr;
	}

	public String getPsyj() {
		return psyj;
	}

	public void setPsyj(String psyj) {
		this.psyj = psyj;
	}


}
