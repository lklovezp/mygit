.main{ position:relative; overflow:hidden; background:#ffffff;}
.main.padding{padding-left:230px;}
.main .l_part{width:230px; position:absolute; left:0; top:0; overflow:auto; background:#f6f6f6;user-select:none;-webkit-user-select:none;}

.main .content{ width:100%; background-image:url(../images/home/content_bg.png); background-repeat:repeat-y;}
.main .l_part a{ text-decoration:none; display:block; font-size:14px; height:40px; line-height:40px;
     padding-left:30px; padding-right:30px; margin-right:6px; overflow:hidden;}
.main .l_part .onlySecond{ height:40px; background:#cff1ff;border-bottom:1px solid #b8d0da; cursor:pointer;}
.main .l_part .onlySecond a{ color:#0c4079;cursor:pointer;}
.main .l_part .onlySecond.cur{ background:#00a2d9;}
.main .l_part .onlySecond.cur a{ background:#5fd5ff; color:#ffffff;}
.main .l_part .onlySecond:hover{ font-weight:bold;}

.main .l_part .thirdParent{ height:40px; background:#cff1ff;border-bottom:1px solid #b8d0da;cursor:pointer;}
.main .l_part .thirdParent a{color:#0c4079;cursor:pointer;}
.main .l_part .thirdParent.open a{background:url(../images/home/subNav_open.png) right center no-repeat; }
.main .l_part .thirdParent.close a{background:url(../images/home/subNav_close.png) right center no-repeat;}
.main .l_part .thirdParent:hover{ font-weight:bold;}

.main .l_part .third p{ height:36px; background:#e7f2f6;border-bottom:1px solid #b8d0da; cursor:pointer;}
.main .l_part .third p a{color:#0c4079; height:36px; line-height:36px; font-size:14px;cursor:pointer;}
.main .l_part .third p.cur{ background:#00a2d9;}
.main .l_part .third p.cur a{ background:#5fd5ff; color:#ffffff;}
.main .l_part .third p:hover{ font-weight:bold;}



